{
  "name": "bizplanner-sass-compile",
  "version": "1.0.0",
  "description": "SASS compiling for our CSS assets.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass-dev": "sass --watch --update --style=expanded web/app/themes/hello-elementor-child/lib/scss:web/app/themes/hello-elementor-child/lib/css",
    "sass-prod": "sass --no-source-map --style=compressed web/app/themes/hello-elementor-child/lib/scss:web/app/themes/hello-elementor-child/lib/css"
  },
